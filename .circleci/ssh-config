  Hostname 54.168.66.12
  User ec2-user
  StrictHostKeyChecking=no

Host playground-web
  Hostname ipaddress
  User ubuntu
  StrictHostKeyChecking=no
  ProxyCommand ssh -W %h:%p playground-bastion

